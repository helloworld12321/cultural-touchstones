{
  "name": "cultural-touchstones-client",
  "version": "0.0.0",
  "description": "A web app to keep track of movies you've been meaning to watch.",
  "main": "dist/elm.js",
  "scripts": {
    "build": "mkdir -p dist && npm run build:html && npm run build:sass && npm run build:elm",
    "build:html": "cp index.html dist/",
    "build:sass": "sass styles:dist/styles -I node_modules/",
    "build:elm": "elm make src/Main.elm --output=dist/elm.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helloworld12321/cultural-touchstones.git"
  },
  "keywords": [
    "Movies",
    "Film",
    "Lists"
  ],
  "author": "Joe Moonan Walbran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helloworld12321/cultural-touchstones/issues"
  },
  "homepage": "https://github.com/helloworld12321/cultural-touchstones#readme",
  "devDependencies": {
    "elm-test": "^0.19.1-revision2",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "sakura.css": "^1.3.1"
  }
}
